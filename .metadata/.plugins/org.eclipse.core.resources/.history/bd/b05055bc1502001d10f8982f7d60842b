package p1.models;

import java.util.ArrayList;
import java.util.List;

public class Cook {
private String name, username, password;
int id;
List<Recipe> recipes = new ArrayList<Recipe>();


public Cook(String name, String username, String password) {
	List<Recipe> allRecipes = new ArrayList<Recipe>();
	this.name = "Cook";
	this.username = "cooker10";
	this.password = "password4";
	this.id = 1;
	
	Recipe recipe1 = new Recipe("Cake", "Fluggy Cake", "Flour, Eggs, Butter", 30),
		   recipe2 = new Recipe("Hot dogs", "Glorious Glizzys", "Hot dogs, buns", 5);
	
	allRecipes.add(recipe1);
	allRecipes.add(recipe2);
}

public String getName() {
	return name;
}
public void setName(String name) {
	this.name = name;
}
public String getUsername() {
	return username;
}
public void setUsername(String username) {
	this.username = username;
}
public String getPassword() {
	return password;
}
public void setPassword(String password) {
	this.password = password;
}
public int getId() {
	return id;
}
public void setId(int id) {
	this.id = id;
}
public List<Recipe> getRecipes() {
	return recipes;
}
public void setRecipes(List<Recipe> recipes) {
	this.recipes = recipes;
}


}
