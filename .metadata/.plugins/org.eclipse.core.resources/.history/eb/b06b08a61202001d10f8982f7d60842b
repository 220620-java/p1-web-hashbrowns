package p1.models;

public class Recipe {
private String  name, description, ingriedients;
private int id;


public Recipe(String name, String desc, String ing) {
	this.name = name;
	this.description = desc;
	this.ingriedients = ing;
}


public String getName() {
	return name;
}
public void setName(String name) {
	this.name = name;
}
public String getIngriedients() {
	return ingriedients;
}
public void setIngriedients(String ingriedients) {
	this.ingriedients = ingriedients;
}
public int getId() {
	return id;
}
public void setId(int id) {
	this.id = id;
}
public String getDescription() {
	return description;
}
public void setDescription(String description) {
	this.description = description;
}


public void add(Recipe recipeObj) {
	// TODO Auto-generated method stub
	
}

}
